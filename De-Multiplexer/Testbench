module demux_tb();

	// Inputs
	reg [2:0] s;
	reg enable;

	// Output
	wire [7:0] y;
	integer i;
	
	// Instantiation and port mapping
	demux uut (
		.s(s), 
		.enable(enable),
		.y(y)
	);
		
        
	// Stimulus generation
    initial
	    begin
		enable=0;
		s = 3'b000;
		#20;
		enable = 1;
		for(i=0;i<8;i=i+1)
			begin
				s=i;
				#20;
			end
		
		#10 $finish;
		end 
	initial 
	  begin
	  $monitor("enable=%b, s=%b, y=%b",enable,s,y);
	  end
endmodule
